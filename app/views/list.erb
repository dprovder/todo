<%= @list.name %>

<!-- <div id="start"><button>start!</button></div> -->
<div>
		<form method="POST" action = "/lists/<%=@list.id%>/create_task">
			<input type= "text" name = "name">
			<input type="submit">
		</form>
</div>


<div class="this">
	<%if @incompleted!=[]%>	
		<form id="tally" method="POST" action= "/lists/<%=@list.id%>">
			<%@incompleted.each do |task|%>
				<ul>	
  <li>
    <div class="circle"></div>
    <h4><%=task.name%></h4>
    <input id="<%=task.id%>" type="checkbox" name="done[]" value= "<%=task.id%>"/>
<!--     <a href="/lists/<%=task.id%>/delete_task">delete</a> -->
    <label for="<%=task.id%>"><i class="fa fa-check"></i></label>
  </li>
</ul>	
			<%end%>
			<input style="position: absolute; left: -9999px; width: 1px; height: 1px;" tabindex="-1" type="submit"></p>
		</form>
		<%end%>
			
			<%@completed.each do |t|%>
				<div class="input-group"><p><strike><%=t.name%></strike></p></div>
			<%end%>
			
</div>

